<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard1.3;net462</TargetFrameworks>
    <Version>1.0.0</Version>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <FileVersion>1.0.0.0</FileVersion>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Description>This package is example purposes only. It is not intended to be used in production.
This package is generated out of the csproj `Project Sdk='Microsoft.NET.Sdk'`.</Description>
    <PackageTags>example;do-not-use;</PackageTags>
    <Product>example only - do not use it in production</Product>
    <Company>Kistler</Company>
    <Authors>Michal Svoboda</Authors>
    <RepositoryUrl>https://github.com/net-ba/NuGetInCSharp.git</RepositoryUrl>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)'=='Release'">
    <OutputPath>..\bin\Release\</OutputPath>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)'=='Debug'">
    <OutputPath>..\bin\Debug\</OutputPath>
  </PropertyGroup>
</Project>